
{
  "expo": {
   
   
    "name": "TOKA",
    "slug": "TOKA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./src/app/screens/multicolor.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/app/screens/androidicon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      
      
      "build":{
        "babel":{
          "include": ["rn-sliding-up-panel", "static-container"]

        }
      }
    }
  }
}
